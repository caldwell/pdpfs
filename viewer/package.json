{
  "name": "pdpfs-shim",
  "version": "0.1.0",
  "description": "",
  "main": "electron.cjs",
  "type": "module",
  "scripts": {
    "build": "npm run build-rs && npm run build-web",
    "build-rs": "cargo-cp-artifact -nc pdpfs.node -- cargo build --message-format=json-render-diagnostics",
    "build-web": "esbuild web/pdpfs.js --sourcemap --metafile=bundle-meta.json --outfile=web/pdpfs.bundle.js --external:'/assets/*' --bundle --target=chrome118",
    "build-debug": "npm run build --",
    "build-release": "npm run build -- --release",
    "install": "npm run build-release",
    "test": "cargo test",
    "launch": "./node_modules/.bin/electron --inspect electron.cjs",
    "build-launch": "npm run build && npm run launch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@caldwell/jsml": "^1.1.5",
    "@eslint/eslintrc": "^2.1.4",
    "bootstrap": "^5.3.0-alpha1",
    "cargo-cp-artifact": "^0.1",
    "electron": "^27.1.0",
    "esbuild": "^0.19.8",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^5.0.0-canary-5dd35968b-20231201",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "sass": "^1.58.1"
  }
}
